{
  "repositoryUrl" : "https://github.com/pawellabaj/auto-record.git",
  "branches": [
    "main"
  ],
  "tagFormat": "v${version}",
  "dryRun": true,
  "debug": true,
  "plugins": [
    "semantic-release-export-data",
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "releaseRules": [
          {
            "type": "build",
            "scope": "deps",
            "release": "patch"
          }
        ]
      }
    ]
  ]
}
